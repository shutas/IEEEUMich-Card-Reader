#!/bin/bash
#
# Executable: test
#
# Style Check Script for reader.py
#
# Written by: Shuta Suzuki (shutas@umich.edu)

# Stop on Errors
set -e

echo ""
echo "<=== STARTING TEST SCRIPT ===>"
echo ""

# Runs pycodestyle on reader.py
pycodestyle reader.py

# Runs pydocstyle on reader.py
pydocstyle reader.py

# Runs pylint on reader.py
pylint --reports=n --disable=cyclic-import reader.py

echo "<=== ALL TESTS PASSED ===>"
echo ""