#!/bin/bash
#
# Executable: test
#
# Style Check Script for reader.py
#
# Written by: Shuta Suzuki (shutas@umich.edu)

# Stop on Errors
set -e

echo ""
echo "<=== STARTING TEST SCRIPT ===>"
echo ""

# Runs pycodestyle
pycodestyle ..

# Runs pydocstyle
pydocstyle ..

# Runs pylint
pylint --reports=n --disable=cyclic-import ..

echo "<=== ALL TESTS PASSED ===>"
echo ""